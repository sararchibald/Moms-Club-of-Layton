<% [1,2,3,4,5] %>
<% @pages.each do |page| %>
  <div class="parallax-container">
    <div class="parallax grey lighten-5">
      <div class="row">
        <div class="col s10 offset-s1">
          <h1><%= page.title %></h1>
          <%= page.content(:content).try(:html_safe) %>
        </div>
      </div>
    </div>
  </div>
  <div class="parallax-container" style="box-shadow:-10px 0 47px #000 inset;">
    <div class="parallax">
      <%= image_tag("#{page.id}.jpg", style: 'width:100%') %>
    </div>
  </div>
<% end %>
